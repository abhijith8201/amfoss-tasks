
#include <iostream>
using namespace std;

int main()
{
	int N,M,i;
	int count=0;
	cin >> N;
	cin >> M;
	int numbers[M];
	int num;
	
  for ( i = 1; i <= M ; i++)
  {
	 
    if (i % 2 == 1)
    {
		
      count++ ;
      
     
    }
	else
		continue;

	 
	
  }
  cout << count<<endl;
 for ( int j = 1; j <= M ; j++)
  {
	 
    if (j % 2 == 1)
    {
		num = j;
      count++ ;
      //numbers[i] = num ;
     
    }
	else
		continue;

	 numbers[j] = num ;
	cout << numbers[j] << " ";
  }
  

  return 0;
}
