

#include <iostream>
using namespace std;
int main(int argc, char **argv)
{
	int numbooks;
	int count;
	cin>>numbooks;
	
	for(int i =1;i<=numbooks;i++)
	{
		for(int j=1;j<numbooks;j++)
		{
		if(i!=j)
		{
		
			if(i*j == numbooks)
			{
				count++;
			}
			if(j*i == numbooks)
			{
				count++;
			}
		}
		else
		{
			if(i*j == numbooks)
			{
				count++;
			}
		}
		}
	}
	cout<<count;
	
	return 0;
}


